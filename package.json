    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "globals": "^16.5.0",
    "gulp": "^5.0.1",
    "limescape-docs": "file:/home/rodger/truelime-git-projects/limescape-ai-limescape-docs/limescape-docs-1.22.1.tgz",
    "n8n-core": "^1.122.1",
    "n8n-workflow": "^1.120.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@n8n/node-cli": "^0.17.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  }
