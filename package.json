{
  "name": "n8n-nodes-limescape-docs",
  "version": "1.22.1",
  "description": "N8N node for Limescape Docs OCR & Document Extraction",
  "keywords": [
    "n8n-community-node-package",
    "vision",
    "multi-modal",
    "document",
    "document-processing",
    "limescape",
    "limescape-docs",
    "ocr",
    "document-extraction",
    "vision-models"
  ],
  "license": "MIT",
  "homepage": "https://limescape.ai",
  "author": {
    "name": "Rodger Blom",
    "email": "rodger.blom@truelime.nl"
